{
    "name": "bbhkxd/helper",
    "description": "A helper component for hyperf.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "helper"
    ],
    "support": {
    },
    "require": {
        "php": ">=7.2",
        "hyperf/http-server": ">1.1.0",
        "hyperf/utils": "~1.1.0"
    },
    "require-dev": {
        "hyperf/di": "~1.1.0",
        "phpunit/phpunit": "^7.0.0",
        "friendsofphp/php-cs-fixer": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "bbhkxd\\Helper\\": "src/"
        },
        "files": [
            "Helper/helper.php"
        ]
    },
    "bin": [
    ],
    "scripts": {
        "cs-fix": "php-cs-fixer fix $1",
        "test": "phpunit --colors=always"
    }
}
